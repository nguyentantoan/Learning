<h1>Categories</h1>
<table>
  <thead>
    <tr>
      <th>Name</th>
    </tr>
  </thead>

  <tbody>
    <% @categories.each do |category| %>
      <tr>
        <td><%= category.name %></td>
        </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Back to home', root_path,class:"btn btn-warning" %>
<%=link_to 'Create new category',new_admin_category_path,class:"btn btn-warning" %>
