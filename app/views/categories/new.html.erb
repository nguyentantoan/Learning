<h1>Category</h1>
<%= form_for Category.new, url: categories_path do |f| %>

<div class="form-group">
  <%= f.label :name %>
  <%= f.text_field :name, class: 'form-control',placeholder:"Enter name" %>
</div>

<%= f.submit 'Submit', class: 'btn btn-primary' %>
<% end %>
<%= link_to 'Back to home', root_path %>
